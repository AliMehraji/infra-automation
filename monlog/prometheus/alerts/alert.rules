groups:
- name: Prometheus_Alerts
  rules:
  - alert: PrometheusExporterDown
    expr: up == 0
    for: 5m
    labels:
      severity: error
    annotations:
      summary: "Prometheus Exporter down (instance {{ $labels.instance }})"
      description: "Prometheus exporter down\n  VALUE = {{ $value }}\n  LABELS: {{ $labels }}"
